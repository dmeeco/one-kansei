{
  "name": "11straps",
  "version": "0.4.0",
  "description": "simple 11ty and Bootstrap5 boilerplate",
  "license": "MIT",
  "private": "true",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "main": "index.html",
  "scripts": {
    "dev": "tinacms dev -c \"npm run serve\"",
    "build": "npm run clean-prod && npm run build:assets && npm run build:eleventy && npm run build:tina",
    "build:eleventy": "eleventy",
    "build:assets": "gulp dist-assets && gulp sass",
    "build:tina": "tinacms build",
    "build-dev": "eleventy && gulp dist-assets && gulp sass",
    "11ty": "eleventy && gulp inject-css",
    "sync": "gulp browser-sync",
    "watch-dev": "npm run build-dev && npm run sync",
    "postinstall": "gulp copy-assets",
    "watch:all": "onchange 'src/' -- npm run build-dev",
    "serve": "eleventy --serve",
    "watch": "npm run clean && npm run build-dev && run-p sync watch:all",
    "clean": "gulp clean-dev",
    "clean-prod": "gulp clean-public",
    "dev:tina": "tinacms dev",
    "dev:11ty": "eleventy --serve"
  },
  "dependencies": {
    "@11ty/eleventy": "2.0.1",
    "bootstrap": "5.3.0",
    "browser-sync": "^3.0.4",
    "clean-css": "^5.3.0",
    "gulp": "^5.0.1",
    "gulp-clean": "^0.4.0",
    "gulp-clean-css": "^4.2.0",
    "gulp-dart-sass": "^1.0.2",
    "gulp-htmlmin": "^5.0.1",
    "gulp-imagemin": "^9.1.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "npm-watch": "^0.11.0",
    "watch": "^0.13.0"
  },
  "author": "Holger Koenemann",
  "devDependencies": {
    "@11ty/eleventy-img": "^3.1.0",
    "@11ty/eleventy-navigation": "^0.3.3",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@tinacms/cli": "^1.10.0",
    "@types/node": "^24.0.10",
    "concurrently": "^9.2.0",
    "gulp-html-replace": "^1.6.2",
    "gulp-purgecss": "^4.1.3",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.0.2",
    "react-is": "^18.1.0",
    "sass": "^1.89.2",
    "tinacms": "^2.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dmeeco/one-kansei"
  },
  "keywords": [],
  "bugs": {
    "url": ""
  },
  "homepage": "https://kansei.one"
}